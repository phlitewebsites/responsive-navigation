{
  "name": "responsive-nav",
  "version": "0.0.5",
  "description": "lean off canvas navigation",
  "main": "index.js",
  "scripts": {
    "build:scripts": "browserify -p bundle-collapser/plugin index.js | uglifyjs > build/nav.js && npm run build:test",
    "build:test": "browserify -s responsiveNavigation -p bundle-collapser/plugin index.js | uglifyjs > build/demo.js",
    "watch:scripts": "watch \"npm run build:scripts\" lib",
    "build:styles": "gulp styles",
    "watch:styles": "gulp watch:styles",
    "build:": "npm run build:scripts && npm run build:styles",
    "watch": "parallelshell \"npm run watch:scripts\" \"npm run watch:styles\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pythonic1/responsive-navigation.git"
  },
  "keywords": [
    "offcanvas",
    "menu",
    "nav",
    "navigation",
    "responsive",
    "css3",
    "browserify"
  ],
  "author": {
    "name": "Pythonic",
    "email": "pythonic1@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pythonic1/responsive-navigation/issues"
  },
  "homepage": "https://github.com/pythonic1/responsive-navigation",
  "dependencies": {
    "clas": "0.0.9",
    "lodash.foreach": "^2.4.1"
  },
  "devDependencies": {
    "browserify": "^8.1.1",
    "bundle-collapser": "^1.1.1",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-minify-css": "^0.3.13",
    "gulp-sass": "^1.3.2",
    "parallelshell": "^1.0.3",
    "supports.transform": "git://github.com/pythonic1/supports.transform",
    "uglify-js": "^2.4.16",
    "watch": "^0.13.0"
  }
}
